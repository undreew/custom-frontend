{
	"name": "scratch-react",
	"version": "1.0.0",
	"description": "A simple React project setup with Webpack and Babel",
	"main": "index.js",
	"scripts": {
		"plop": "plop --plopfile ./etc/plop/plop.js",
		"start": "nodemon server/server.js",
		"start:dev": "node -r dotenv/config server/server.js dotenv_config_path=.env.dev",
		"start:prod": "node -r dotenv/config server/server.js dotenv_config_path=.env.prod",
		"build": "cross-env NODE_ENV=production webpack --config etc/webpack.prod.config.js",
		"build:analyze": "cross-env NODE_ENV=production webpack-bundle-analyzer dist/**/*.js",
		"serve:prod": "npm run build && npm run start:prod",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@babel/register": "^7.28.3",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router-dom": "^6.30.1"
	},
	"devDependencies": {
		"@babel/core": "^7.28.4",
		"@babel/preset-env": "^7.28.3",
		"@babel/preset-react": "^7.27.1",
		"autoprefixer": "^10.4.14",
		"babel-loader": "^10.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^7.1.2",
		"css-minimizer-webpack-plugin": "^5.0.0",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.7.0",
		"nodemon": "^3.1.10",
		"plop": "^4.0.4",
		"postcss": "^8.4.0",
		"postcss-loader": "^7.3.0",
		"style-loader": "^4.0.0",
		"terser-webpack-plugin": "^5.3.0",
		"webpack": "^5.101.3",
		"webpack-bundle-analyzer": "^4.9.0",
		"webpack-cli": "^6.0.1",
		"webpack-dev-middleware": "^7.4.5",
		"webpack-dev-server": "^5.2.2",
		"webpack-hot-middleware": "^2.26.1",
		"webpackbar": "^7.0.0"
	}
}
